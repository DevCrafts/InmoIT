{
  "AllowedHosts": "*",
  "CacheSettings": {
    "SlidingExpiration": 2
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Hangfire": "Warning"
    }
  },
  "PersistenceSettings": {
    "UseMsSql": true,
    "connectionStrings": {
      "mssql": "Data Source=.;Initial Catalog=InmoITDB;Integrated Security=True;MultipleActiveResultSets=True"
    }
  },
  "CorsSettings": {
    "Url": "http://localhost:4200"
  },
  "SerializationSettings": {
    "UseSystemTextJson": true,
    "UseNewtonsoftJson": false
  },
  "ApplicationSettings": {
    "ApiUrl": "https://localhost:5000/"
  }
}
